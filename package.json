{
  "name": "next-lesscode",
  "version": "5.1.3",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./function": {
      "types": "./dist/function/index.d.ts",
      "import": "./dist/function/index.js",
      "require": "./dist/function/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    },
    "./regex": {
      "types": "./dist/regex/index.d.ts",
      "import": "./dist/regex/index.js",
      "require": "./dist/regex/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "crypto-js": "^4.1.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.12.10",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "tsup": "^8.4.0",
    "typescript": "^5.4.5"
  },
  "description": "This is a package to encapsulate irritating code into functions, regex, and many more.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makecoderun/code-less.git"
  },
  "author": "coderun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makecoderun/code-less/issues"
  },
  "homepage": "https://github.com/makecoderun/code-less#readme"
}